class Solution {
public:
    string reverseWords(string s) {
        stringstream ss(s);
        vector<string>words;
        string word;
        while(ss>>word){
            
    words.push_back(word);
        }
    string reversed;
        for(int i=words.size()-1;i>=0;i--){
            reversed+=words[i]+" ";
            }
        reversed.pop_back();
        return reversed;
        }
   
};